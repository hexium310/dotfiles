root = true

[*]
charset = utf-8
end_of_line = lf
indent_style = space
indent_size = 2
insert_final_newline = true
trim_trailing_whitespace = true

[{.zshrc,.zshenv,*.zsh,*.py}]
indent_size = 4

[config/git/config]
indent_style = tab

[config/ime_dic/*.txt]
indent_style = tab
insert_final_newline = false
trim_trailing_whitespace = false
